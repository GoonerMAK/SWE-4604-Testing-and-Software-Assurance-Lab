			"name": "Get Safety Products",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Getting Safety Products\", function () {",
							"    pm.response.to.have.status(200); ",
							"",
							"    var responseBody = pm.response.json();",
							"",
							"    pm.expect(responseBody.data).to.be.an('array').that.is.not.empty;",
							"",
							"    responseBody.data.forEach(function(product) {",
							"        pm.expect(product.name.toLowerCase()).to.include('safety');",
							"    });",
							"",
							"    pm.environment.set(\"Safety\", responseBody.data[0].id);",
							"});",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
